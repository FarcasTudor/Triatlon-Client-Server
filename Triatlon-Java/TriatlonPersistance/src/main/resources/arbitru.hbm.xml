<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Arbitru"
           table="arbitru">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="nume"
                column="nume"/>
        <property
                name="prenume"
                column="prenume"/>
        <property
                name="parola"
                column="parola"/>
        <property
                name="username"
                column="username"/>
    </class>
</hibernate-mapping>